<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="700.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SellListController">
   <children>
      <VBox prefHeight="700.0" prefWidth="302.0" style="-fx-background-color: #121212;">
         <children>
            <TextField fx:id="buyPlayerText" prefHeight="38.0" prefWidth="217.0" promptText="Player Name" style="-fx-background-radius: 100;">
               <VBox.margin>
                  <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="Maiandra GD" size="14.0" />
               </font>
            </TextField>
            <Button fx:id="buyButton" mnemonicParsing="false" onAction="#BuyPlayer" style="-fx-background-radius: 100;" text="Buy Player">
               <font>
                  <Font name="Maiandra GD" size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backToClub" style="-fx-background-radius: 100;" text="Back">
               <font>
                  <Font name="Maiandra GD" size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="80.0" left="80.0" right="80.0" top="80.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Label fx:id="labelPlayers" layoutX="326.0" prefHeight="665.0" prefWidth="947.0" style="-fx-background-color: #d6eef8;">
         <font>
            <Font name="Maiandra GD" size="28.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
